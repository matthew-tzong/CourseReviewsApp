<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.sde.reviews.LogInSceneController">
   <children>
      <TextField fx:id="usernameField" layoutX="225.0" layoutY="153.0" />
      <TextField fx:id="passwordField" layoutX="225.0" layoutY="238.0" />
      <Text layoutX="244.0" layoutY="136.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
         <font>
            <Font name="Californian FB" size="27.0" />
         </font>
      </Text>
      <Text layoutX="244.0" layoutY="217.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
         <font>
            <Font name="Californian FB" size="27.0" />
         </font>
      </Text>
      <Button fx:id="closeButton" layoutX="540.0" layoutY="9.0" mnemonicParsing="false" onAction="#handleCloseButton" text="Close" />
      <Button fx:id="loginButton" layoutX="200.0" layoutY="285.0" mnemonicParsing="false" onAction="#handleLoginButton" text="Log-In" />
      <Button fx:id="createLoginButton" layoutX="325.0" layoutY="285.0" mnemonicParsing="false" onAction="#handleCreateLoginButton" text="Create New User" />
      <Label fx:id="messageLabel" layoutX="114.0" layoutY="344.0" textFill="RED" />
      <Text layoutX="85.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Course Reviews" textAlignment="CENTER">
         <font>
            <Font name="Bodoni MT Black Italic" size="52.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
